{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "roll": {
      "type": "uid",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ],
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "general",
        "obc",
        "sc",
        "st",
        "ews",
        "pwd"
      ]
    },
    "institute_email_id": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "personal_email_id": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "resume": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "resume_link": {
      "type": "string"
    },
    "mobile_number_1": {
      "type": "biginteger"
    },
    "mobile_number_2": {
      "type": "biginteger"
    },
    "program": {
      "type": "enumeration",
      "enum": [
        "B.Tech",
        "M.Tech",
        "M.Sc",
        "BS"
      ],
      "required": true
    },
    "date_of_birth": {
      "type": "date",
      "required": true
    },
    "X_marks": {
      "type": "decimal",
      "required": true
    },
    "XII_marks": {
      "type": "decimal",
      "required": true
    },
    "rank": {
      "type": "string",
      "required": true
    },
    "categoryRank": {
      "type": "string",
      "required": true
    },
    "admission_year": {
      "type": "string",
      "required": true
    },
    "approved": {
      "type": "enumeration",
      "enum": [
        "created",
        "pending",
        "approved",
        "rejected"
      ],
      "default": "pending",
      "required": true
    },
    "spi_1": {
      "type": "string"
    },
    "spi_2": {
      "type": "string"
    },
    "spi_3": {
      "type": "string"
    },
    "spi_4": {
      "type": "string"
    },
    "spi_5": {
      "type": "string"
    },
    "spi_6": {
      "type": "string"
    },
    "spi_7": {
      "type": "string"
    },
    "spi_8": {
      "type": "string"
    },
    "cpi": {
      "type": "string",
      "required": true
    },
    "bachelor_marks": {
      "type": "string"
    },
    "master_marks": {
      "type": "string"
    },
    "department": {
      "type": "enumeration",
      "enum": [
        "mathematics",
        "computer science "
      ],
      "required": true
    },
    "course": {
      "type": "enumeration",
      "enum": [
        "Mathematics & Computing",
        "Mechatronics"
      ],
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "user_relation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "registered_for": {
      "type": "enumeration",
      "enum": [
        "FTE",
        "Internship"
      ],
      "required": true
    },
    "profile_pic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "remarks": {
      "type": "text",
      "required": false,
      "private": true
    }
  }
}
