{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "roll": {
      "type": "uid",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Others"
      ],
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "General",
        "OBC",
        "SC",
        "ST",
        "ews",
        "pwd"
      ],
      "required": true
    },
    "institute_email_id": {
      "type": "email"
    },
    "personal_email_id": {
      "type": "email"
    },
    "password": {
      "type": "password"
    },
    "resume": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "resume_link": {
      "type": "string"
    },
    "mobile_number": {
      "type": "biginteger"
    },
    "mobile_number_alternate": {
      "type": "biginteger"
    },
    "program": {
      "type": "enumeration",
      "enum": [
        "B.Tech",
        "M.Tech",
        "M.Sc",
        "BS"
      ],
      "required": true
    },
    "date_of_birth": {
      "type": "date"
    },
    "approved": {
      "type": "boolean"
    },
    "X_percentage": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 100
    },
    "XII_percentage": {
      "type": "decimal",
      "max": 100,
      "min": 0,
      "required": true
    },
    "rank": {
      "displayName": "rank",
      "type": "component",
      "repeatable": false,
      "component": "rank-details.rank"
    },
    "registered_for": {
      "type": "enumeration",
      "enum": [
        "FTE",
        "Internship"
      ],
      "required": true
    },
    "department": {
      "type": "string",
      "required": true
    },
    "course": {
      "type": "string",
      "required": true
    },
    "current_sem": {
      "type": "integer",
      "min": 1
    },
    "ug_college": {
      "type": "string"
    },
    "ug_cpi": {
      "type": "decimal"
    },
    "spi": {
      "displayName": "spi",
      "type": "component",
      "repeatable": false,
      "component": "spi-details.spi"
    },
    "cpi": {
      "type": "decimal",
      "required": true
    },
    "applied_jobs": {
      "displayName": "Applied Job",
      "type": "component",
      "repeatable": true,
      "component": "applied-job-status.applied-job"
    },
    "profile_profile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "activated": {
      "type": "boolean",
      "default": true
    },
    "other_achievements": {
      "type": "text"
    },
    "placed_status": {
      "type": "boolean",
      "default": false
    },
    "projects": {
      "displayName": "Project",
      "type": "component",
      "repeatable": true,
      "component": "project.project"
    }
  }
}
